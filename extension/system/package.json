{
  "name": "apptron-system",
  "displayName": "Apptron",
  "publisher": "progrium",
  "description": "",
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other",
    "Themes"
  ],
  "enabledApiProposals": [
    "ipc"
  ],
  "activationEvents": [
    "onFileSystem:wanix"
  ],
  "extensionKind": [
    "web"
  ],
  "browser": "./dist/web/extension.js",
  "scripts": {
    "compile-web": "npm run check-types && node esbuild.js",
    "watch-web": "node esbuild.js --watch",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@vscode/test-web": "^0.0.54",
    "esbuild": "^0.25.9",
    "typescript": "^5.4.5"
  },
  "contributes": {
    "themes": [
      {
        "label": "Tractor Dark",
        "uiTheme": "tractor-dark",
        "path": "./themes/Tractor-color-theme.json"
      }
    ],
    "commands": [
      {
        "command": "apptron.open",
        "title": "Open File or Folder"
      }
    ]
  },
  "dependencies": {
    "@progrium/duplex": "^0.2.1"
  }
}
