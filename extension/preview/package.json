{
  "name": "apptron-preview",
  "displayName": "Preview",
  "publisher": "progrium",
  "description": "",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:preview.openBrowser"
  ],
  "browser": "./dist/web/extension.js",
  "scripts": {
    "compile-web": "npm run check-types && node esbuild.js",
    "watch-web": "node esbuild.js --watch",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@vscode/test-web": "^0.0.54",
    "esbuild": "^0.21.5",
    "typescript": "^5.4.5"
  },
  "contributes": {
    "commands": [
      {
        "command": "preview.openBrowser",
        "title": "Open Browser Preview",
        "category": "Preview"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "preview.openBrowser",
          "when": "true"
        }
      ]
    }
  }
}